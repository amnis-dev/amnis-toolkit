{
  "name": "@reduxjs/toolkit",
  "version": "2.0.0-beta.1",
  "description": "The official, opinionated, batteries-included toolset for efficient Redux development",
  "author": "Mark Erikson <mark@isquaredsoftware.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reduxjs/redux-toolkit.git"
  },
  "keywords": [
    "redux",
    "react",
    "starter",
    "toolkit",
    "reducer",
    "slice",
    "immer",
    "immutable",
    "redux-toolkit"
  ],
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/reduxToolkit.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/reduxToolkit.mjs",
      "default": "./dist/reduxToolkit.js"
    },
    "./nanoid": {
      "types": "./dist/nanoid/index.d.ts",
      "import": "./dist/reduxToolkitNanoid.mjs",
      "default": "./dist/reduxToolkitNanoid.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/reduxToolkitReact.mjs",
      "default": "./dist/reduxToolkitReact.js"
    },
    "./query": {
      "types": "./dist/query/index.d.ts",
      "import": "./dist/reduxToolkitQuery.mjs",
      "default": "./dist/reduxToolkitQuery.js"
    },
    "./query/react": {
      "types": "./dist/query/react/index.d.ts",
      "import": "./dist/reduxToolkitQueryReact.mjs",
      "default": "./dist/reduxToolkitQueryReact.js"
    },
    "./immer": {
      "types": "./dist/immer/index.d.ts",
      "import": "./dist/immer.mjs",
      "default": "./dist/immer.js"
    },
    "./redux": {
      "types": "./dist/redux/index.d.ts",
      "import": "./dist/redux.mjs",
      "default": "./dist/redux.js"
    },
    "./redux-thunk": {
      "types": "./dist/redux-thunk/index.d.ts",
      "import": "./dist/reduxThunk.mjs",
      "default": "./dist/reduxThunk.js"
    },
    "./reselect": {
      "types": "./dist/reselect/index.d.ts",
      "import": "./dist/reselect.mjs",
      "default": "./dist/reselect.js"
    }
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.36.4",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.1.5",
    "@types/convert-source-map": "^1.5.1",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^10.14.4",
    "@types/query-string": "^6.3.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "axios": "^0.19.2",
    "console-testing-library": "0.6.1",
    "convert-source-map": "^1.7.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^9.1.0",
    "invariant": "^2.2.4",
    "jsdom": "^21.0.0",
    "json-stringify-safe": "^5.0.1",
    "msw": "^0.40.2",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "query-string": "^7.0.1",
    "rimraf": "^3.0.2",
    "size-limit": "^4.11.0",
    "tslib": "^1.10.0",
    "tsx": "^3.12.2",
    "typescript": "~4.9",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "yargs": "^15.3.1"
  },
  "scripts": {
    "run-build": "vite build --mode production",
    "build": "echo Bundling modules... && yarn run-build && echo Compiling TS... && tsc -p tsconfig.build.json",
    "build-only": "yarn run-build",
    "format": "prettier --write \"(src|examples)/**/*.{ts,tsx}\" \"**/*.md\"",
    "format:check": "prettier --list-different \"(src|examples)/**/*.{ts,tsx}\" \"docs/*/**.md\"",
    "lint": "eslint src examples",
    "test": "vitest",
    "api": "yarn api:rtk && yarn api:rtk:react && yarn api:rtk:query && yarn api:rtk:query:react",
    "api:rtk": "api-extractor run -c api-extractor.jsonc",
    "api:rtk:react": "api-extractor run --verbose -c api-extractor-react.jsonc",
    "api:rtk:query": "api-extractor run --verbose -c api-extractor-query.jsonc",
    "api:rtk:query:react": "api-extractor run --verbose -c api-extractor-query-react.jsonc",
    "type-tests": "tsc -p src/tests/tsconfig.typetests.json && tsc -p src/query/tests/tsconfig.typetests.json",
    "prepack": "yarn build"
  },
  "files": [
    "dist/",
    "query",
    "react",
    "nanoid"
  ],
  "dependencies": {
    "immer": "^10.0.2",
    "redux": "^5.0.0-beta.0",
    "redux-thunk": "^3.0.0-beta.0",
    "reselect": "^5.0.0-alpha.2"
  },
  "peerDependencies": {
    "react": "^16.9.0 || ^17.0.0 || ^18",
    "react-redux": "^7.2.1 || ^8.0.2 || ^9.0.0-alpha.1"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-redux": {
      "optional": true
    }
  },
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/reduxjs/redux-toolkit/issues"
  },
  "homepage": "https://redux-toolkit.js.org"
}
